{
  "name": "bi-model",
  "version": "1.0.0",
  "description": "Bi-Model of the Bi-FrameWork",
  "main": ".compiled/src/index.js",
  "scripts": {
    "start": "node .compiled/src/runTestServer.js",
    "dev": "cross-env NODE_ENV=dev nodemon -e js,graphql,json --exec babel-node src/runTestServer.js",
    "compile": "rm -rf .compiled && babel src --out-dir .compiled/src",
    "test": "jest",
    "testDev": "jest --watch",
    "testCoverage": "jest --coverage",
    "testCoverageDev": "jest --watch --coverage",
    "openTestCoverageReport": "jest --coverage; open ./coverage/lcov-report/index.html",
    "openTestCoverageReportDev": "open ./coverage/lcov-report/index.html;jest --watch --coverage"
  },
  "quokka": {
    "babel": true
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": false,
    "mapCoverage": true
  },
  "author": "BinPar Team S.L:",
  "license": "MIT",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "cross-env": "^5.0.5",
    "express": "^4.15.3",
    "faker": "^4.1.0",
    "fibers": "^2.0.0",
    "moment": "^2.18.1",
    "mongoose": "^4.11.7",
    "mongoose-simple-random": "^0.4.1",
    "validator": "^8.0.0",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "graphql": "^0.10.5",
    "graphql-date": "^1.0.3",
    "graphql-tools": "^1.1.0",
    "jsonwebtoken": "^7.4.3",
    "apollo-server-express": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-transform-exponentiation-operator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "regenerator-runtime": "^0.11.0"
  }
}
